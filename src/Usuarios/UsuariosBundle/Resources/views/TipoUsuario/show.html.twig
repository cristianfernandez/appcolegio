{% extends 'TemasTemasBundle::admin.html.twig' %}

{% block article %}
    <h1>TipoUsuario</h1>

    <table class="table table-striped table-bordered">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Idusuario</th>
                <td>{{ entity.idUsuario }}</td>
            </tr>
            <tr>
                <th>Nombretipousuario</th>
                <td>{{ entity.nombreTipoUsuario }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('tipousuario') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('tipousuario_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
