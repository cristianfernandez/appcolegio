{% extends 'TemasTemasBundle::admin.html.twig' %}

{% block article %}

    <h1>Estudiante</h1>

    <table class="table table-striped table-bordered">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Idtipoidentificacion</th>
                <td>{{ entity.idTipoIdentificacion }}</td>
            </tr>
            <tr>
                <th>Primernombre</th>
                <td>{{ entity.primerNombre }}</td>
            </tr>
            <tr>
                <th>Segundonombre</th>
                <td>{{ entity.segundoNombre }}</td>
            </tr>
            <tr>
                <th>Primerapellido</th>
                <td>{{ entity.primerApellido }}</td>
            </tr>
            <tr>
                <th>Segundoapellido</th>
                <td>{{ entity.segundoApellido }}</td>
            </tr>
            <tr>
                <th>Telefono</th>
                <td>{{ entity.telefono }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ entity.email }}</td>
            </tr>
            <tr>
                <th>Nui</th>
                <td>{{ entity.nui }}</td>
            </tr>
            <tr>
                <th>Nuip</th>
                <td>{{ entity.nuip }}</td>
            </tr>
            <tr>
                <th>Numerodocumento</th>
                <td>{{ entity.numeroDocumento }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('estudiante') }}">
            Volver a la lista
        </a>
    </li>
    <li>
        {#
        <a href="{{ path('estudiante_edit', { 'id': entity.id }) }}">
            Editar
            
        </a>#}
        <a href="{{ path('estudiante_edit', { 'id': entity.id }) }}" class="btn btn-small btn-success"><i class="btn-icon-only icon-ok"> </i></a>
    </li>
    <a href="javascript:;" class="btn btn-danger btn-small"><i class="btn-icon-only icon-remove"> </i></a>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
