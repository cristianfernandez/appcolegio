{% extends 'TemasTemasBundle::admin.html.twig' %}

{% block article %}
    <h1>Docente</h1>

    <table class="table table-striped table-bordered">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Idsede</th>
                <td>{{ entity.idSede }}</td>
            </tr>
            <tr>
                <th>Idmateria</th>
                <td>{{ entity.idMateria }}</td>
            </tr>
            <tr>
                <th>Nombres</th>
                <td>{{ entity.nombres }}</td>
            </tr>
            <tr>
                <th>Apellidos</th>
                <td>{{ entity.apellidos }}</td>
            </tr>
            <tr>
                <th>Telefono</th>
                <td>{{ entity.telefono }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ entity.email }}</td>
            </tr>
            <tr>
                <th>Idtipoidentificacion</th>
                <td>{{ entity.idTipoIdentificacion }}</td>
            </tr>
            <tr>
                <th>Numeroidentificacion</th>
                <td>{{ entity.numeroIdentificacion }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ entity.estado }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('docente') }}">
            Volver a la lista
        </a>
    </li>
    <li>
        <a href="{{ path('docente_edit', { 'id': entity.id }) }}">
            Editar
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
