{% extends 'TemasTemasBundle::admin.html.twig' %}

{% block article %}

    <h1>Docente list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Idsede</th>
                <th>Idmateria</th>
                <th>Nombres</th>
                <th>Apellidos</th>
                <th>Telefono</th>
                <th>Email</th>
                <th>Idtipoidentificacion</th>
                <th>Numeroidentificacion</th>
                <th>Estado</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('docente_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.idSede }}</td>
                <td>{{ entity.idMateria }}</td>
                <td>{{ entity.nombres }}</td>
                <td>{{ entity.apellidos }}</td>
                <td>{{ entity.telefono }}</td>
                <td>{{ entity.email }}</td>
                <td>{{ entity.idTipoIdentificacion }}</td>
                <td>{{ entity.numeroIdentificacion }}</td>
                <td>{{ entity.estado }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('docente_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('docente_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('docente_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
