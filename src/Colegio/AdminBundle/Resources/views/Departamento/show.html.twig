{% extends 'TemasTemasBundle::admin.html.twig' %}

{% block article %}
    <h1>Departamento</h1>

    <table class="table table-striped table-bordered">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombre departamento</th>
                <td>{{ entity.nombreDepartamento }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('departamento') }}">
           Regresar
        </a>
    </li>
    <li>
        <a href="{{ path('departamento_edit', { 'id': entity.id }) }}">
            Editar
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
