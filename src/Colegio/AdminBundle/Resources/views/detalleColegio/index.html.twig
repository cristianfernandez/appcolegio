{% extends 'TemasTemasBundle::admin.html.twig' %}

{% block article %}
    <h1>detalleColegio list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Idcolegio</th>
                <th>Idsede</th>
                <th>Actualyear</th>
                <th>Capacidades</th>
                <th>Discapacidades</th>
                <th>Modeloeducativo</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('detallecolegio_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.idColegio }}</td>
                <td>{{ entity.idSede }}</td>
                <td>{{ entity.actualYear }}</td>
                <td>{{ entity.capacidades }}</td>
                <td>{{ entity.discapacidades }}</td>
                <td>{{ entity.modeloEducativo }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('detallecolegio_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('detallecolegio_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('detallecolegio_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
