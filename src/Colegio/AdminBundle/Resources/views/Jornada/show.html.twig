{% extends 'TemasTemasBundle::admin.html.twig' %}

{% block article %}
    <h1>Jornada</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Jornada</th>
                <td>{{ entity.jornada }}</td>
            </tr>
            <tr>
                <th>Horario</th>
                <td>{{ entity.horario }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('jornada') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('jornada_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
