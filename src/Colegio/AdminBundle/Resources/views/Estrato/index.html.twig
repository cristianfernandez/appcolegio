{% extends 'TemasTemasBundle::admin.html.twig' %}

{% block article %}
    <h1>Listar Estrato</h1>

    <table class="table table-striped table-bordered">
        <thead>
            <tr>
                <th>Id</th>
                <th>Estrato</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('estrato_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.estrato }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('estrato_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('estrato_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('estrato_new') }}">
                Crear nuevo estrato
            </a>
        </li>
    </ul>
    {% endblock %}
